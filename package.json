{
  "name": "tower32",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently --raw \"npm run frontend:build\" \"npm run backend:build\"",
    "backend:build": "nest build",
    "backend:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "backend:lint": "tslint -p tsconfig.json -c tslint.json",
    "backend:start": "nest start",
    "backend:start:debug": "nest start --debug --watch",
    "backend:start:dev": "nest start --watch",
    "backend:start:prod": "node dist/main",
    "backend:test": "jest",
    "backend:test:cov": "jest --coverage",
    "backend:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "backend:test:e2e": "jest --config ./test/jest-e2e.json",
    "backend:test:watch": "jest --watch",
    "frontend:build": "vue-cli-service build",
    "frontend:inspect": "vue-cli-service inspect",
    "frontend:lint": "vue-cli-service lint",
    "frontend:start": "vue-cli-service serve",
    "prebuild": "rimraf dist",
    "start": "NODE_ENV=production npm run backend:start:prod",
    "start:dev": "concurrently --raw \"npm run frontend:start\" \"npm run backend:start:debug\""
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/platform-fastify": "^6.8.5",
    "@nestjs/typeorm": "^6.2.0",
    "axios": "^0.19.0",
    "bulma": "^0.7.5",
    "bulma-extensions": "^6.2.7",
    "core-js": "^2.6.5",
    "dotenv": "^8.2.0",
    "fastify-static": "^2.5.0",
    "http-proxy-middleware": "^0.20.0",
    "inline-chunk-manifest-html-webpack-plugin": "^2.0.0",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "nunjucks": "^3.2.0",
    "point-of-view": "^3.6.0",
    "qs": "^6.9.0",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.6.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rimraf": "^3.0.0",
    "roboto-fontface": "*",
    "rxjs": "^6.5.3",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.21",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-slider-component": "^3.0.41",
    "vuetify": "^2.1.14",
    "vuex": "^3.0.1",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@vue/cli": "^3.11.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^5.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.18.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
