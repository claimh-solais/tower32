{
  "name": "tower32",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently --raw \"npm run frontend:build\" \"npm run backend:build\"",
    "backend:build": "nest build",
    "backend:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "backend:lint": "tslint -p tsconfig.json -c tslint.json",
    "backend:start": "nest start",
    "backend:start:debug": "nest start --debug --watch",
    "backend:start:dev": "nest start --watch",
    "backend:start:prod": "node dist/main",
    "backend:test": "jest",
    "backend:test:cov": "jest --coverage",
    "backend:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "backend:test:e2e": "jest --config ./test/jest-e2e.json",
    "backend:test:watch": "jest --watch",
    "frontend:build": "vue-cli-service build",
    "frontend:inspect": "vue-cli-service inspect",
    "frontend:lint": "vue-cli-service lint",
    "frontend:start": "vue-cli-service serve",
    "prebuild": "rimraf dist",
    "start": "NODE_ENV=production npm run backend:start:prod",
    "start:dev": "concurrently --raw \"npm run frontend:start\" \"npm run backend:start:debug\""
  },
  "dependencies": {
    "@fastify/view": "7.1.0",
    "@mdi/font": "7.0.96",
    "@nestjs/common": "7.6.18",
    "@nestjs/core": "7.6.18",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/platform-fastify": "7.6.18",
    "@nestjs/typeorm": "7.1.5",
    "axios": "0.27.2",
    "bulma": "0.9.4",
    "bulma-extensions": "6.2.7",
    "core-js": "2.6.12",
    "dotenv": "16.0.2",
    "fastify-static": "4.4.1",
    "http-proxy-middleware": "0.20.0",
    "inline-chunk-manifest-html-webpack-plugin": "2.0.0",
    "lodash": "4.17.15",
    "mysql": "2.17.1",
    "nunjucks": "3.2.0",
    "point-of-view": "4.16.0",
    "qs": "6.9.0",
    "reflect-metadata": "0.1.13",
    "register-service-worker": "1.6.2",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "rimraf": "3.0.0",
    "roboto-fontface": "*",
    "rxjs": "7.5.6",
    "sqlite3": "4.2.0",
    "typeorm": "0.2.45",
    "vue": "2.7.10",
    "vue-router": "3.6.5",
    "vue-slider-component": "3.0.41",
    "vuetify": "2.1.14",
    "vuex": "3.6.2",
    "webpack": "4.46.0",
    "webpack-manifest-plugin": "2.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.18",
    "@types/express": "4.17.1",
    "@types/jest": "24.9.0",
    "@types/node": "14.18.28",
    "@types/supertest": "2.0.8",
    "@vue/cli": "3.12.1",
    "@vue/cli-plugin-babel": "3.12.1",
    "@vue/cli-plugin-eslint": "3.12.1",
    "@vue/cli-plugin-pwa": "3.12.1",
    "@vue/cli-service": "3.12.1",
    "@vue/runtime-dom": "3.2.39",
    "babel-eslint": "10.0.1",
    "concurrently": "7.4.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.3",
    "jest": "24.9.0",
    "node-sass": "4.14.1",
    "prettier": "2.7.1",
    "sass": "1.32.13",
    "sass-loader": "8.0.2",
    "supertest": "6.2.4",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.14.1",
    "tslint": "5.20.1",
    "typescript": "4.7.4",
    "vue-cli-plugin-vuetify": "2.0.2",
    "vue-template-compiler": "2.7.10",
    "vuetify-loader": "1.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
